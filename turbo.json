{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [
    ".stylelintrc.json",
    "eslint.config.js",
    "postcss.config.js"
  ],
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "dev": {
      "cache": false
    },
    "format": {
      "outputs": []
    },
    "lint": {
      "outputs": []
    }
  }
}