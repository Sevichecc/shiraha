{
  "name": "vitepress-theme-shiraha",
  "version": "0.0.1-alpha.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "run-s build:vars build:themes",
    "build:vars": "postcss src/vars.sss --base src --ext css -d dist",
    "build:themes": "postcss src/themes/*.sss --base src --ext css -d dist"
  },
  "main": "./dist/vars.css",
  "exports": {
    ".": "./dist/vars.css",
    "./*": "./dist/themes/*.css"
  }
}
